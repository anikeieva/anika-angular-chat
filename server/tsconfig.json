{
  "files": [
    "src/*.ts",
    "src/model/*.ts",
    "src/data-base/*.ts"
  ],
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "es6", "dom", "es2015.promise"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "compileOnSave":false,
  "include": [
    "src/*.ts",
    "src/model/*.ts",
    "src/data-base/*.ts"
  ],
  "exclude": [ "node_modules" ]
}